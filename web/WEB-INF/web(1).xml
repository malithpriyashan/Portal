<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addevent</servlet-name>
        <servlet-class>Addevent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IPostAnswer</servlet-name>
        <servlet-class>IPostAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SPostAnswer</servlet-name>
        <servlet-class>SPostAnswer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/servlet/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addevent</servlet-name>
        <url-pattern>/Addevent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IPostAnswer</servlet-name>
        <url-pattern>/IPostAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SPostAnswer</servlet-name>
        <url-pattern>/SPostAnswer</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>register.html</welcome-file>
    </welcome-file-list>
</web-app>
